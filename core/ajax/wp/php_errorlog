      LEFT JOIN (
              SELECT postmeta_1.meta_key , postmeta_1.meta_value ,postmeta_1.post_id
              from wp_postmeta as postmeta_1
              WHERE (postmeta_1.meta_key = '_sku' )
            ) as postmeta_table_1 on postmeta_table_1.post_id = wp_posts.ID
            
        WHERE wpka_posts.post_status = 'publish' AND wpka_posts.post_type = 'product' AND wpka_posts.post_title like '%'    
				ORDER BY post_date ASC
