					details.name as product_type,
					attribute_details.attribute_details, 
					variation_id_table.variation_ids as variation_ids 
					FROM wpka_posts
				#Categories
				LEFT JOIN
    (
        SELECT postsAsTaxo.ID, GROUP_CONCAT(category_terms.name) as category_names, GROUP_CONCAT(category_terms.term_id) as category_ids
						FROM wpka_posts postsAsTaxo
						LEFT JOIN wpka_term_relationships category_relationships ON (postsAsTaxo.ID = category_relationships.object_id)
						LEFT JOIN wpka_term_taxonomy category_taxonomy ON (category_relationships.term_taxonomy_id = category_taxonomy.term_taxonomy_id)
						LEFT JOIN wpka_terms category_terms ON (category_taxonomy.term_id = category_terms.term_id)
						WHERE category_taxonomy.taxonomy = 'product_cat' 
						 AND category-names  = 6  # AND category_terms.term_id IN (6)
						GROUP BY postsAsTaxo.ID
					) as tblCategories ON tblCategories.ID = wpka_posts.ID
				
				#Link in product type
				LEFT JOIN
    (
        SELECT a.ID, d.name FROM wpka_posts a
						LEFT JOIN wpka_term_relationships b ON (a.ID = b.object_id)
						LEFT JOIN wpka_term_taxonomy c ON (b.term_taxonomy_id = c.term_taxonomy_id)
						LEFT JOIN wpka_terms d ON (c.term_id = d.term_id)
						WHERE c.taxonomy = 'product_type'
					) as details ON details.ID = wpka_posts.ID
				
				
				#Attributes in detail
				LEFT JOIN
    (
        SELECT a.ID, GROUP_CONCAT(CONCAT(c.taxonomy, '=', d.slug, '=', d.name)) as attribute_details
						FROM wpka_posts a
						LEFT JOIN wpka_term_relationships b ON (a.ID = b.object_id)
						LEFT JOIN wpka_term_taxonomy c ON (b.term_taxonomy_id = c.term_taxonomy_id)
						LEFT JOIN wpka_terms d ON (c.term_id = d.term_id)
						WHERE c.taxonomy LIKE 'pa\_%'
						GROUP BY a.ID
					) as attribute_details ON attribute_details.ID = wpka_posts.ID

				#variations
				LEFT JOIN
    (
        SELECT GROUP_CONCAT(postvars.id) as variation_ids, postvars.post_parent
						FROM wpka_posts postvars
						WHERE (postvars.post_type = 'product_variation') AND (postvars.post_status = 'publish')
						GROUP BY postvars.post_parent
					) as variation_id_table on variation_id_table.post_parent = wpka_posts.ID
        WHERE wpka_posts.post_status = 'publish' AND wpka_posts.post_type = 'product' AND wpka_posts.post_title like '%'
				ORDER BY post_date ASC
